<Window x:Class="wpfAppEntity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfAppEntity"
        xmlns:y="clr-namespace:wpfAppEntity.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <y:ucLogin x:Name="ucLoginForm" success="UcLoginForm_success" fail="UcLoginForm_fail" ></y:ucLogin>
        <y:ucGridView x:Name="ucGridViewForm" Visibility="Hidden"></y:ucGridView>
    </Grid>
</Window>
